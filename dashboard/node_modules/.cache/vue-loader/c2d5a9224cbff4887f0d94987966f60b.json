.fetchLine(this.selected);\r\n  },\r\n  methods: {\r\n    fetchLine: function (conn) {\r\n      var vm = this;\r\n      var key = conn.key;\r\n      try {\r\n        webcall.get(conn.url).then(async function (response) {\r\n          var temp = await JSON.parse(JSON.stringify(response.data));\r\n          alert(response.data);\r\n          var data = {};\r\n          for (var m of temp) {\r\n            data[m[key]] = parseInt(m[\"count\"]);\r\n          }\r\n          vm.result = data;\r\n          console.log(vm.result);\r\n        });\r\n      } catch (err) {\r\n        console.log(\"error\");\r\n        alert(err);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}